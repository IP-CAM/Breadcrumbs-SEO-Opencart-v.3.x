    <file path="catalog/view/theme/*/template/product/search.twig" error="skip">
        <operation error="skip">
            <search><![CDATA[<ul class="breadcrumb">]]></search>
            <add position="replace"><![CDATA[<ul class="breadcrumb" vocab="http://schema.org/" typeof="BreadcrumbList">]]></add>
        </operation>
    </file>
    <file path="catalog/view/theme/*/template/product/search.twig" error="skip">
        <operation error="skip">
            <search><![CDATA[<li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>]]></search>
            <add position="replace"><![CDATA[<li property="itemListElement" typeof="ListItem">{% if loop.last %}<span property="name">{{ breadcrumb.text }}</span>{% else %}<a href="{{ breadcrumb.href }}" property="item" typeof="WebPage"><span property="name">{{ breadcrumb.text }}</span></a>{% endif %}<meta property="position" content="{{ loop.index }}"/></li>]]></add>
        </operation>
    </file>

